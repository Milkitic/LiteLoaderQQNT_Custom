{
    "manifest_version": 4,
    "type": "extension",
    "name": "Chii DevTools",
    "slug": "chii_devtools",
    "description": "使用 Chii 进行远程调试",
    "version": "1.0.0",
    "authors": [
        {
            "name": "沫烬染",
            "link": "https://github.com/mo-jinran"
        }
    ],
    "platform": [
        "win32",
        "linux",
        "darwin"
    ],
    "injects": {
        "renderer": "./src/renderer.js",
        "main": "./src/main.js",
        "preload": "./src/preload.js"
    },
    "repository": {
        "repo": "mo-jinran/chii-devtools",
        "branch": "v4",
        "release": {
            "tag": "1.0.0",
            "file": "chii-devtools.zip"
        }
    }
}